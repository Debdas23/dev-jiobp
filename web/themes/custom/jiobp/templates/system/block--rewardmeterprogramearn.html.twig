<section class="earn" style="margin-top:0px !important;">
<div class="coins"></div>
<div class="coins1"></div>
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12 align-self-center">
                <h1 class="block_info--title" data-aos="fade-up">
                    {{ content.field_etitle.0 }}
                </h1>
                <span class="pill_underline mb-4" data-aos="fade-up"></span>
                <p class="subtitle" data-aos="fade-up">{{ content.field_etext.0 }}</p>
            </div>
            {% if content.field_earn_list %}
                {% for key, item in content.field_earn_list if key|first != '#' %}
                <div class="col-lg-4 col-md-4">
                    <div class="earn--card mt-0" data-aos="fade-up">
                        <p class="top-subtitle" data-aos="fade-up">{{ item['#paragraph'].field_etitle[0].value|raw }}</p>
                        <span class="feature-num" data-aos="fade-up">{{ item['#paragraph'].field_point[0].value|raw }}</span>
                        <p class="bottom-subtitle" data-aos="fade-up">
                            {{ item['#paragraph'].field_esub_text[0].value|raw }}
                        </p>
                        <p class="card-desc" data-aos="fade-up">
                           {{ item['#paragraph'].field_per_text[0].value|raw }}
                        </p>
                    </div>
                </div>
                {% endfor %}
            {% endif %}
        </div>
    </div>
</section>