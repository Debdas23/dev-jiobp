<style>
.beverages .col-md-6:nth-child(n+3) {
    margin-top: 60px;
}
</style>
<section>
    <div class="container beverages">
        <div class="row text-center">
        {% if content.field_fgallery %}
            {% for key, item in content.field_fgallery if key|first != '#' %}
            <div class="col-md-6">
                <img src="{{ file_url(item['#paragraph'].field_csupload_image.entity.fileuri) }}" alt="Delights" class="img-fluid beverages_img" data-aos="fade-up" />
            </div>
            {% endfor %}
        {% endif %}
        </div>
    </div>
</section>